<?xml version="1.0" encoding="UTF-8"?>
<svg width="1200" height="630" viewBox="0 0 1200 630" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="1200" height="630" fill="url(#bgGradient)"/>
  
  <!-- Decorative circles -->
  <circle cx="100" cy="100" r="150" fill="#60a5fa" opacity="0.2"/>
  <circle cx="1100" cy="530" r="200" fill="#93c5fd" opacity="0.1"/>
  
  <!-- Logo circle background -->
  <circle cx="300" cy="315" r="120" fill="#ffffff" opacity="0.1"/>
  
  <!-- Shopping cart icon -->
  <g transform="translate(250, 280)">
    <!-- Cart body -->
    <path d="M20 60 L30 20 L90 20 L100 60 Z" stroke="#ffffff" stroke-width="3" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
    <!-- Cart wheels -->
    <circle cx="35" cy="65" r="8" stroke="#ffffff" stroke-width="3" fill="none"/>
    <circle cx="95" cy="65" r="8" stroke="#ffffff" stroke-width="3" fill="none"/>
    <!-- Handle -->
    <path d="M30 20 Q60 -10 90 20" stroke="#ffffff" stroke-width="3" fill="none" stroke-linecap="round"/>
  </g>
  
  <!-- AI Brain icon -->
  <g transform="translate(850, 280)">
    <!-- Brain outline -->
    <path d="M30 20 Q20 30 20 50 Q20 70 40 75 Q60 70 60 50 Q60 30 50 20 Q40 15 30 20" stroke="#ffffff" stroke-width="2.5" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
    <!-- Neural connections -->
    <circle cx="25" cy="45" r="3" fill="#ffffff"/>
    <circle cx="35" cy="35" r="3" fill="#ffffff"/>
    <circle cx="45" cy="45" r="3" fill="#ffffff"/>
    <circle cx="55" cy="40" r="3" fill="#ffffff"/>
    <!-- Connection lines -->
    <line x1="25" y1="45" x2="35" y2="35" stroke="#ffffff" stroke-width="1.5" opacity="0.6"/>
    <line x1="35" y1="35" x2="45" y2="45" stroke="#ffffff" stroke-width="1.5" opacity="0.6"/>
    <line x1="45" y1="45" x2="55" y2="40" stroke="#ffffff" stroke-width="1.5" opacity="0.6"/>
  </g>
  
  <!-- Text -->
  <text x="600" y="320" font-family="Arial, sans-serif" font-size="72" font-weight="bold" fill="#ffffff" text-anchor="middle">
    CogniCart
  </text>
  <text x="600" y="380" font-family="Arial, sans-serif" font-size="36" fill="#93c5fd" text-anchor="middle">
    AI-Powered Smart Shopping
  </text>
  
  <!-- Tagline -->
  <text x="600" y="450" font-family="Arial, sans-serif" font-size="24" fill="#ffffff" text-anchor="middle" opacity="0.9">
    Experience Intelligent Commerce
  </text>
</svg>
